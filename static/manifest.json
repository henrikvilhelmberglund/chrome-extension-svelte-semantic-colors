{
	"name": "Semantic Colors",
	"description": "A browser extension made with Svelte Kit",
	"version": "0.0.1",
	"manifest_version": 3,
	"permissions": ["activeTab"],
	"action": {
		"default_title": "Svelte Extension",
		"default_icon": "favicon.png",
		"default_popup": "index.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["semantic-colors.css"]
		}
	],
	"icons": {
		"48": "favicon.png"
	}
}
